<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
        "http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
    <package name="jiang" extends="struts-default" >
        <action name="login" class="com.jiang.LoginAction">
            <result name="success">/success.jsp</result>
            <result name="error">/Login.jsp</result>
        </action>
    </package>
    <package name="regist" extends="struts-default">
        <action name="regist" class="com.jiang.LoginAction" method="regist">
            <result name="success">/Login.jsp</result>
            <result name="error">/zhuce.jsp</result>
        </action>
    </package>
</struts>